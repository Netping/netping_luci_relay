	option name 'Relay 3'
	option embedded '0'
	option state '0'
	option status '0'
	option restart_time '30'
	option proto 'first'
	option dest_port '80'
	option dest_addr '0.0.0.0'
	option login 'user'
	option password 'pass'
	option number '0'

