
config globals 'globals'
	list proto 'first.Протокол 1'
	list proto 'second.Протокол 2'
	list proto 'third.Протокол 3'
	list state '0.Off'
	list state '1.On'
	list status '0.Норма'
	list status '1.Нет связи'
	list status '2.Перезагрузка'
	list status '3.Ошибка'
	option restart_time '30'
	option default_port '80'
	option default_name 'Relay'
	option relay_max_number '16'

config relay 'prototype'
	option name 'Relay'
	option embedded '0'
	option state '0'
	option status '0'
	option restart_time '30'
	option proto 'first'
	option dest_port '80'
	option dest_addr '0.0.0.0'
	option login 'user'
	option password 'pass'
	option number '0'

config relay
	option name 'Relay 1'
	option embedded '1'
	option state '0'
	option status '0'
	option restart_time '30'

